{
  "name": "mealer-backend",
  "version": "1.0.0",
  "description": "Mealer App Backend",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/server.ts",
    "db:start": "docker compose up -d db",
    "db:stop": "docker compose down",
    "db:create": "kysely-ctl create",
    "db:mig": "tsx src/cli/migrate.ts",
    "db:rollback": "kysely-ctl rollback",
    "db:gen": "kysely-codegen --out-file src/database/types.ts"
  },
  "keywords": [],
  "author": "Adam Koziorz",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "esm": "^3.2.25",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "kysely": "^0.28.9",
    "kysely-ctl": "^0.19.0",
    "pg": "^8.16.3",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.3",
    "@types/pg": "^8.15.5",
    "kysely-codegen": "^0.19.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
